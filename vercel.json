{
    "name": "Abhisarga Ticketing System",
    "version": 2,
        "builds": [{
        "src": "server.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9", "installCommand": "pip install -r vercel-requirements.txt" }
    }],
    "routes": [
        { "src": "(.*)", "dest": "server.py" }
    ]
}